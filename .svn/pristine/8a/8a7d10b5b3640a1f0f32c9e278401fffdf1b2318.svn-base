package y1j2x34.wechat.state.model;

import java.io.Serializable;
/**
 * 状态模型
 * @author yangjianxin
 * @date 2014-12-9 上午8:24:38
 */
public class WechatStateModel implements Serializable{
	private static final long serialVersionUID = 1L;
	private Integer stateId;
	private String stateName;
	private Integer actionId;
	private Integer stateType;
	public WechatStateModel() {
	}
	public WechatStateModel(Integer stateId) {
		super();
		this.stateId = stateId;
	}
	public WechatStateModel(Integer stateId, String stateName, Integer actionId, Integer stateType) {
		super();
		this.stateId = stateId;
		this.stateName = stateName;
		this.actionId = actionId;
		this.stateType = stateType;
	}
	public String getStateName() {
		return stateName;
	}
	public Integer getActionId() {
		return actionId;
	}
	public Integer getStateId() {
		return stateId;
	}
	public void setActionId(Integer actionId) {
		this.actionId = actionId;
	}
	public void setStateId(Integer stateId) {
		this.stateId = stateId;
	}
	public void setStateName(String stateName) {
		this.stateName = stateName;
	}
	public void setStateType(Integer stateType) {
		this.stateType = stateType;
	}
	public Integer getStateType() {
		return stateType;
	}
}
