package y1j2x34.wechat.pojo;
/**
 * 分组模型基类
 */
public class BaseGroup {
	private int id;
	private String name;
	/**
	 * 分组编号
	 * @return
	 */
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	/**
	 * 分组名称
	 * @return
	 */
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
}
