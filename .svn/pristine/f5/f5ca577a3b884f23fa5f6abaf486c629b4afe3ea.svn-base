package y1j2x34.wechat.message.req;

import y1j2x34.wechat.utils.MessageUtils;

/**
 * 文本消息
 * <hr/>
 * @author 杨建新
 * @see ReqBaseMessage
 */
public class ReqTextMessage extends ReqBaseMessage{

	private static final long serialVersionUID = 6121623035784741296L;
	private String Content;
	public ReqTextMessage() {
		setMsgType(MessageUtils.RESP_MESSAGE_TYPE_TEXT);
	}
	/**
	 * @return 文本消息内容
	 * @see #setContent(String)
	 */
	public String getContent() {
		return Content;
	}
	/**
	 * @param content
	 * @see #getContent()
	 */
	public void setContent(String content) {
		this.Content = content;
	}
}
